<%= form_for @participants, :html => {:multipart => true} do |form| %>

  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  
  <div>
    <%= form.file_field :picture %>
  </div>
  
  <div>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>
  
  <%= form.submit "Next Button" %>
  
<% end %>
